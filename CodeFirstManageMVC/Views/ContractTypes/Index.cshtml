@model IEnumerable<EntityFramework_CodeFirst_Example.Model.ContractType>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2>Index</h2>
<p>@ViewBag.Count</p>

<p>
    @Html.ActionLink("Create New", "Create")
</p>

@if (ViewBag.SuccessMessage != null)
{
    <i class="alert alert-success">@ViewBag.SuccessMessage</i>
}
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ContractTypeName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MinValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaxValue)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ContractTypeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MinValue)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxValue)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
